<AnchorPane id="AnchorPane" prefHeight="563.0" prefWidth="366.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="typingrobot.controllers.FXML_Preferences_Controller">
   <children>
      <VBox layoutX="52.0" layoutY="83.0" prefHeight="480.0" prefWidth="473.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="83.0">
         <children>
            <CheckBox fx:id="addEnterCheckBox" mnemonicParsing="false" onAction="#onCheckBoxAction" styleClass="paddingLarge" stylesheets="@../resources/smm_stylesheet.css" text="Add another ENTER after row end" />
            <CheckBox fx:id="addDoubleEnterCheckBox" mnemonicParsing="false" onAction="#onCheckBoxAction" styleClass="paddingLarge" stylesheets="@../resources/smm_stylesheet.css" text="Add double ENTER after row end" />
            <CheckBox fx:id="addArrowDownCheckBox" mnemonicParsing="false" onAction="#onCheckBoxAction" styleClass="paddingLarge" stylesheets="@../resources/smm_stylesheet.css" text="Add ARROW_DOWN after all to begin new row" />
