            <CheckBox fx:id="typeInvoiceInDscCheckBox" mnemonicParsing="false" onAction="#onCheckBoxAction" styleClass="paddingLarge" stylesheets="@../resources/smm_stylesheet.css" text="Type invoice in description" />
         </children>
      </VBox>
      <Label layoutX="52.0" layoutY="32.0" text="Typing preferences">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="okButton" layoutX="270.0" layoutY="504.0" mnemonicParsing="false" onAction="#onOkClick" prefHeight="25.0" prefWidth="60.0" text="OK" />
   </children>
</AnchorPane>
